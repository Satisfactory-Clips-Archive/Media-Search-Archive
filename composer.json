{
	"name": "satisfactory-clips-archive/video-clip-notes",
	"description": "Notes for twitch clips",
	"type": "project",
	"license": "Apache-2.0",
	"authors": [
		{
			"name": "SignpostMarv",
			"email": "me@signpostmarv.name"
		}
	],
	"autoload-dev": {
		"files": [
			"./app/src-dev/funcs.php",
			"./app/src/funcs.php"
		],
		"psr-4": {
			"SignpostMarv\\VideoClipNotes\\": [
				"./tests/",
				"./app/src/"
			]
		}
	},
	"config": {
		"classmap-authoritative": true,
		"sort-packages": true
	},
	"require": {
		"php": "^8.1"
	},
	"require-dev": {
		"cocur/slugify": "^4.0",
		"google/apiclient": "^2.12",
		"masterminds/html5": "^2.7",
		"phpunit/phpunit": "^9.5",
		"psalm/plugin-phpunit": "^0.16.1",
		"roave/security-advisories": "dev-latest",
		"satisfactory-clips-archive/querypath": "^1.0",
		"signpostmarv/php-cs-fixer-config": "^0.22.0",
		"vimeo/psalm": "^4.16"
	}
}
