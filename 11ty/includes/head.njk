<meta property="og:site_name" content="Satisfactory Clips Archive">
<meta property="og:url" content="https://archive.satisfactory.video{{page.url}}">
{%
	if undefined !== transcript
%}<meta property="og:title" content="{{ transcript.dateTitle }} {{ transcript.title }}">{%
	elif undefined !== thread
%}<meta property="og:title" content="Satisfactory Clips Archive - Tweets - {{ thread.title }}">{%
	elif undefined !== topic
%}<meta property="og:title" content="Satisfactory Clips Archive - {{ topic.title }}">{%
	elif undefined !== group
%}<meta property="og:title" content="Satisfactory Clips Archive - {{ group.date_friendly }}">{%
	else
%}<meta property="og:title" content="{{title}}">{%
	endif
%}
<meta name="viewport" content="width=device-width, initial-scale=1">
<link rel="canonical" href="https://archive.satisfactory.video{{page.url}}">
{% if page.url in jsonld %}<meta
	name="description"
	content="{{jsonld[page.url] | jsonld_description }}"
>{% else %}<meta
	name="description"
	content="Serves as an unofficial archive for Clips for Coffee Stain Studios' Satisfactory-related livestreams & other videos"
>{% endif %}
{% for icondata in icon %}
<link
	rel="icon"
	type="{{icondata[1]}}"
	sizes="{{icondata[2]}}x{{icondata[2]}}"
	href="{{icondata[0]}}"
>
{% endfor %}
<link
	rel="icon"
	type="image/png"
	sizes="16x16"
	href="https://i.img.archive.satisfactory.video/favicon.png"
/>
<link
	rel="shortcut icon"
	href="https://i.img.archive.satisfactory.video/favicon.ico"
/>
<link rel="stylesheet" href="/common.css">
{% if '/search/' === page.url %}
<link rel="stylesheet" href="/search.css">
{% else %}
<link rel="stylesheet" href="/browse.css">
{% endif %}
{% if page.url in jsonld %}{% for chunk in jsonld[page.url] %}<script
	type="application/ld+json"
>{{chunk | json | safe}}</script>{% endfor %}{% endif %}
{% if page.url in opengraph %}{% for pair in opengraph[page.url] %}<meta
	property="{{pair[0]}}"
	content="{{pair[1]}}"
>{% endfor %}{% endif %}
{% if page.url in twitter %}{% for key,value in twitter[page.url] %}<meta
	name="{{key}}"
	content="{{value}}"
>{% endfor %}{% endif %}
{% if page.url in tcn %}{% for alt_video_url in tcn[page.url].alts %}<link
	rel="alternate"
	href="{{alt_video_url}}"
>{% endfor %}{% endif %}
{% if transcript?.url in tcn %}{% for alt_video_url in tcn[transcript.url].alts %}<link
	rel="alternate"
	href="{{alt_video_url}}"
>{% endfor %}{% endif %}
<link
	rel="search"
	type="application/opensearchdescription+xml"
	title="Satisfactory Clips Archive - Search"
	href="/ossd.xml"
>
