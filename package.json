    "gulp-clean": "^0.4.0",
    "gulp-htmlmin": "^5.0.1",
    "gulp-imagemin": "^8.0.0",
    "gulp-inline-source": "^4.0.0",
    "gulp-json-transform": "^0.4.7",
    "gulp-libsquoosh": "^1.1.2",
    "gulp-newer": "^1.4.0",
    "gulp-postcss": "^9.0.0",
